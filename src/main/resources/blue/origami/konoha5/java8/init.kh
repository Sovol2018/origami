# java8/init.kh

Bool       = boolean
Int        = int
Float      = double
String     = String

null       = null
true:Bool  = true
false:Bool = false

()            = (%s)
Int->Float        = %s
Float->>Int         = (int)(%s)

0:Int         = %d
0:Float       = %f

+:Int:Int:Int       = %s+%s
+:Float:Float:Float = %s+%s
